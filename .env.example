# NEVER commit .env to git - it contains private keys!

# Your MetaMask private key (export from MetaMask)
# WARNING: Keep this secret! Anyone with this key can access your funds
PRIVATE_KEY=your_private_key_here

# RPC URLs for different networks
SEPOLIA_RPC_URL=https://rpc.sepolia.org
BASE_RPC_URL=https://mainnet.base.org
BASE_TESTNET_RPC_URL=https://goerli.base.org

# API keys for contract verification (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key
BASESCAN_API_KEY=your_basescan_api_key

# Gas reporting (optional)
REPORT_GAS=true